{
    "version": 2,
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ],
    "static": {
        "src": "main/static/",
        "dest": "/static"
    }
}